# Enable Bootmagic Lite
BOOTMAGIC_ENABLE = no
# Mouse keys
MOUSEKEY_ENABLE = no
# Audio control and System control
EXTRAKEY_ENABLE = yes
# Console for debug
CONSOLE_ENABLE = no
# Commands for debug and configuration
COMMAND_ENABLE = yes
# N-Key rollover
NKRO_ENABLE = yes
# Enable keyboard backlight functionality
BACKLIGHT_ENABLE = no
# Audio output
AUDIO_ENABLE = no
# Enable WS2812 RGB underlight
RGBLIGHT_ENABLE = no
