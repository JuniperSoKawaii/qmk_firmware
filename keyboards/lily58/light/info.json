{
    "keyboard_name": "Lily58L",
    "manufacturer": "Keycapsss",
    "url": "https://keycapsss.com",
    "maintainer": "BenRoe",
    "usb": {
        "vid": "0x7983",
        "pid": "0x4C4C",
        "device_version": "1.0.0"
    },
    "features": {
      "rgblight": true
    },
    "encoder": {
        "enabled": true,
        "rotary": [
            {"pin_a": "F4", "pin_b": "F5"}
        ]
    },
    "split": {
        "encoder": {
            "right": {
                "rotary": [
                    {"pin_a": "F5", "pin_b": "F4"}
                ]
            }
        }
    },
    "rgblight": {
        "led_count": 70,
        "pin": "D3",
        "split": true,
        "split_count": [35, 35],
        "max_brightness": 120,
        "saturation_steps": 6,
        "brightness_steps": 6,
        "hue_steps": 6,
        "animations": {
            "rainbow_swirl": true,
            "static_gradient": true,
            "twinkle": true
        }
    },
    "processor": "atmega32u4",
    "bootloader": "caterina"
}
