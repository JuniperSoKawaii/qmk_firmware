# Build Options
# Yes = enable, No = disable

BOOTMAGIC_ENABLE = yes      			# Enable Bootmagic Lite
EXTRAKEY_ENABLE = yes       			# Audio control and System control
COMMAND_ENABLE = yes        			# Commands for debug and configuration
RGBLIGHT_ENABLE = yes					# Enable keyboard RGB underglow
WS2812_DRIVER_REQUIRED = yes			# required for RGBLIGHT to work
WS2812_DRIVER = vendor					# required for RGBLIGHT to work
DEBOUNCE_TYPE = sym_eager_pk			# sym_eager_pk, sym_wyr, sym_hys, sym_eager, sym_wyr_pk, sym_hys_pk
LTO_ENABLE = yes						# Enable Link Time Optimization