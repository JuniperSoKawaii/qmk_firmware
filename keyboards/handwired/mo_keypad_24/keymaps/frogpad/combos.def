// This one is for a reset
COMB(farw_Reset,  TD_RESET,       KC_F, KC_A, KC_R, KC_W)

// Convenience combos
// need an o-e combo to do a backslash, because we already have space-slash for f11
COMB(oe_Bsls,     KC_BSLS,          KC_O, KC_E)

// backspace
COMB(wP_BS,       KC_BSPC,          KC_W, KC_P)
COMB(m9_BS,       KC_BSPC,          KC_9, KC_MINUS)
// tmux
SUBS(uI_TMUX,     SS_LCTL("b"),     KC_U, KC_I)
SUBS(iN_TMUX_LB,  SS_LCTL("b") "[", KC_I, KC_N)
// emacs
SUBS(sY_CX,       SS_LCTL("x"),     KC_S, KC_Y)
SUBS(nY_CX,       SS_LCTL("s"),     KC_N, KC_Y)
// quotes
COMB(fA_Quot,     KC_QUOT,          KC_F, KC_A)
COMB(fR_Dquo,     KC_DQUO,          KC_F, KC_R)
// brackets (including parens)
COMB(eH_Lbrc,     KC_LBRC,          KC_E, KC_H)
COMB(eT_Rbrc,     KC_RBRC,          KC_E, KC_T)
COMB(hT_Lcbr,     KC_LCBR,          KC_H, KC_T)
COMB(hD_Rcbr,     KC_RCBR,          KC_H, KC_D)
COMB(eD_Lprn,     KC_LPRN,          KC_E, KC_D)
COMB(tD_Rprn,     KC_RPRN,          KC_T, KC_D)

