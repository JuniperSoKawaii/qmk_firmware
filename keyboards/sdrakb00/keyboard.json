{
    "manufacturer": "Diego Andres Rabaioli",
    "keyboard_name": "sdrakb00",
    "maintainer": "drabaioli",
    "bootloader": "caterina",
    "diode_direction": "COL2ROW",
    "features": {
        "extrakey": true,
        "mousekey": true,
        "rgb_matrix": true,
        "encoder": true,
        "nkro": true,
        "via": true
    },
    "matrix_pins": {
        "cols": ["F0", "F1", "F4", "F5"],
        "rows": ["D4", "D6", "D7"]
    },
    "processor": "atmega32u4",
    "url": "https://github.com/drabaioli/SdraKb00",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x4200",
        "vid": "0x7331"
    },
    "encoder": {
        "rotary": [
            {"pin_a": "B5", "pin_b": "B6", "resolution": 2}
        ]
    },
    "ws2812": {
      "pin": "B0"
    },
    "rgb_matrix": {
      "driver": "ws2812",
      "max_brightness": 100,
      "sleep": true,
      "timeout": 300000,
      "layout": [
          {"flags": 4, "matrix": [0, 0], "x": 0, "y": 0},
          {"flags": 4, "matrix": [0, 1], "x": 16, "y": 0},
          {"flags": 4, "matrix": [0, 2], "x": 32, "y": 0},
          {"flags": 4, "matrix": [1, 0], "x": 0, "y": 16},
          {"flags": 4, "matrix": [1, 1], "x": 16, "y": 16},
          {"flags": 4, "matrix": [1, 2], "x": 32, "y": 16},
          {"flags": 4, "matrix": [1, 3], "x": 48, "y": 16},
          {"flags": 4, "matrix": [2, 0], "x": 0, "y": 32},
          {"flags": 4, "matrix": [2, 1], "x": 16, "y": 32},
          {"flags": 4, "matrix": [2, 2], "x": 32, "y": 32},
          {"flags": 4, "matrix": [2, 3], "x": 48, "y": 32}
      ],
      "config": {
        "enable": true,
        "keypresses": true,
        "framebuffer_effects": true,
        "modes": [
          ["SOLID_COLOR", 1]
        ]
      }
    },
    "layouts": {
        "LAYOUT_ortho_4x3": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0},
                {"matrix": [0, 1], "x": 1, "y": 0},
                {"matrix": [0, 2], "x": 2, "y": 0},
                {"matrix": [0, 3], "x": 3, "y": 0},
                {"matrix": [1, 0], "x": 0, "y": 1},
                {"matrix": [1, 1], "x": 1, "y": 1},
                {"matrix": [1, 2], "x": 2, "y": 1},
                {"matrix": [1, 3], "x": 3, "y": 1},
                {"matrix": [2, 0], "x": 0, "y": 2},
                {"matrix": [2, 1], "x": 1, "y": 2},
                {"matrix": [2, 2], "x": 2, "y": 2},
                {"matrix": [2, 3], "x": 3, "y": 2}
            ]
        }
    }
}
