// These are to create keys which don't exist in a locale.
// so that we can create alternate maps to qwerty, azerty, or whatever.
// Key name,
// unshifted key and it's required mods.
// the desired shifted keys and it's required mods.

  MK_KEY(DB_1,    BP_DQUO, MOD_LSFT,      BP_DCIR, MOD_LSFT)
  MK_KEY(DB_2,    BP_LDAQ, MOD_LSFT,      BP_AT, MOD_NONE)
  MK_KEY(DB_3,    BP_RDAQ, MOD_LSFT,      BP_DLR, MOD_LSFT)
  MK_KEY(DB_4,    BP_LPRN, MOD_LSFT,      BP_DLR, MOD_NONE)
  MK_KEY(DB_5,    BP_RPRN, MOD_LSFT,      BP_PERC, MOD_NONE)
  MK_KEY(DB_6,    BP_AT, MOD_LSFT,        BP_AT, MOD_BIT(KC_RALT))
  MK_KEY(DB_7,    BP_PLUS, MOD_LSFT,      BP_P, MOD_BIT(KC_RALT))
  MK_KEY(DB_8,    BP_MINS, MOD_LSFT,      BP_ASTR, MOD_NONE)
  MK_KEY(DB_9,    BP_SLSH, MOD_LSFT,      BP_LPRN, MOD_NONE)
  MK_KEY(DB_0,    BP_ASTR, MOD_LSFT,      BP_RPRN, MOD_NONE)
  MK_KEY(DB_GRV,  BP_PERC, MOD_LSFT,      BP_K, MOD_BIT(KC_RALT))
  MK_KEY(DB_SCLN, BP_COMM, MOD_LSFT,      BP_DOT, MOD_LSFT)
  MK_KEY(DB_SLASH, BP_SLSH, MOD_NONE,     BP_QUOT, MOD_LSFT)
  MK_KEY(DB_BSLS, BP_AGRV, MOD_BIT(KC_RALT), BP_B, MOD_BIT(KC_RALT))
  MK_KEY(DB_EQL,  BP_EQL, MOD_NONE,       BP_PLUS, MOD_NONE)
  MK_KEY(DB_COMM, BP_COMM, MOD_NONE,      BP_LDAQ, MOD_BIT(KC_RALT))
  MK_KEY(DB_DOT,  BP_DOT, MOD_NONE,       BP_RDAQ, MOD_BIT(KC_RALT))
  MK_KEY(DB_QUOT, BP_QUOT, MOD_NONE,      BP_DQUO, MOD_NONE)
  MK_KEY(DB_MINS, BP_MINS, MOD_NONE,      KC_SPC, MOD_BIT(KC_RALT))
  MK_KEY(DB_LPRN, BP_LPRN, MOD_NONE,      BP_LPRN, MOD_BIT(KC_RALT))
  MK_KEY(DB_RPRN, BP_RPRN, MOD_NONE,      BP_RPRN, MOD_BIT(KC_RALT))
  MK_KEY(DB_LBRC, BP_Y, MOD_BIT(KC_RALT), BP_LPRN, MOD_BIT(KC_RALT))
  MK_KEY(DB_RBRC, BP_X, MOD_BIT(KC_RALT), BP_RPRN, MOD_BIT(KC_RALT))
  // For the symbol layer
  MK_KEY(DB_HASH, BP_DLR, MOD_LSFT,          BP_DLR, MOD_LSFT)
  MK_KEY(DB_LCBR, BP_LPRN, MOD_BIT(KC_RALT), BP_LPRN, MOD_BIT(KC_RALT))
  MK_KEY(DB_RCBR, BP_LPRN, MOD_BIT(KC_RALT), BP_RPRN, MOD_BIT(KC_RALT))
  MK_KEY(DB_PIPE, BP_B, MOD_BIT(KC_RALT),    BP_B, MOD_BIT(KC_RALT))
  MK_KEY(DB_TILD, BP_K, MOD_BIT(KC_RALT),    BP_K, MOD_BIT(KC_RALT))
  MK_KEY(DB_CIRC, BP_AT, MOD_BIT(KC_RALT),   BP_AT, MOD_BIT(KC_RALT))
  MK_KEY(DB_LESS, BP_LDAQ, MOD_BIT(KC_RALT), BP_LDAQ, MOD_BIT(KC_RALT))
  MK_KEY(DB_GRTR, BP_RDAQ, MOD_BIT(KC_RALT), BP_RDAQ, MOD_BIT(KC_RALT))
  // Keys for BEAKL on Qwerty
  MK_KEY(BQ_DOT,  KC_DOT, MOD_NONE,    KC_2, MOD_LSFT)
  MK_KEY(BQ_COMM, KC_COMMA, MOD_NONE,  KC_1, MOD_LSFT)
  MK_KEY(BQ_QUOT, KC_QUOT, MOD_NONE,   KC_GRV, MOD_NONE)
  // Keys for BEAKL on BEPO
  MK_KEY(BB_DOT,  BP_DOT, MOD_NONE,    BP_AT, MOD_NONE)
  MK_KEY(BB_COMM, BP_COMM, MOD_NONE,   BP_EXLM, MOD_NONE)
  MK_KEY(BB_QUOT, BP_QUOT, MOD_NONE,   BP_PERC, MOD_LSFT)
